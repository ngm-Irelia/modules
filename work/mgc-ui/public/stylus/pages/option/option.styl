@import "../public/component";
@import "../public/laydate";
@import "../../../js/public/jquery-custom-content-scroller/jquery.mCustomScrollbar";

#options_shade {
  position: absolute;
  top: 50px;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: $shade-color-background;
  z-index: 100;
  display: none;
}

#options_box {
  position: absolute;
  top: 12%;
  left: 0;
  width: 100%;
  height: 80%;/*c*/
  display: none;
}

.options_box_title {
  display: inline-block;
  width: 20%;
  height: 40px;
  line-height: 38px;
  font-size: 16px;
  font-weight: 300;
  border: 1px $tabs-item-color-border solid;
  background-color: $tabs-item-color-background-option;
  box-sizing: border-box;
  text-align: center;
  cursor: pointer;
}

#options_box_titles .option_title_active {
  background: $tabs-item-color-background-option-active;
  color: $tabs-item-color-text-acitve;
  border: none;
  border-top: 1px $tabs-item-color-border-top-active solid;
  cursor: default;
}

#options_box_contents {
  height: 90%;/*c*/
}

#person_detail {
  width: 100%;
  margin-right: 12px;
  height: 100%;
}

#options_personal_photo {
  display: inline-block;
  width: 20%;
  margin-right: 12px;
  vertical-align: top;
  padding-top: 4%;
}

#options_personal_photo span {
  font-size: 100px;
}

#options_personal_detail {
  display: inline-block;
  width: 40%;
  height: 100%;
  overflow-y: auto;
  vertical-align: top;
}

#options_personal_account {
  display: inline-block;
  vertical-align: top;
}

#options_personal_detail p,
#options_personal_account p {
  position: relative;
  line-height: 24px;
  font-size: 13px;
}

/*密码的小眼睛*/
#option_modify_detail,
#option_modify_password {
  position: absolute;
  width: 14px;
  height: 14px;
  cursor: pointer;
  display: none;
  font-size: 14px;
}

#option_modify_detail {
  top: 8px;
  right: 60px;
}

#option_modify_password {
  top: 3px;
  right: 30px;
}

/* 改结束 */
.options_personal_detail_title {
  color: $font-color-highlight;
  margin-right: 6px;
}

.personal_title_delegation {
  margin-top: 20px;
}

.personal_title {
  position: relative;
  line-height: 32px;
  color: $font-color-highlight;
  font-weight: 400;
  font-size: 16px;
}

#personal_modify_password,
#personal_modify_detail {
  position: absolute;
  top: 0;
  display: inline-block;
  color: $font-color-highlight;
  cursor: pointer;
  font-size: 12px;
}

.modify_title {
  line-height: 32px;
  color: $font-color-highlight;
  font-weight: 400;
  font-size: 16px;
  text-align: center;
}

#personal_modify_password {
  right: 120px;
}

#personal_modify_detail {
  right: 24px;
}

#person_detail b {
  font-weight: normal;
}

.options_box_content {
  padding: 36px;
  height: 100%;/*c*/
  display: none;
}

.options_message_push {
  border: 1px $border-color solid;
  margin: 0 12px 0 24px;
  padding: 4px 12px;
  outline: none;
  font-size: 14px;
  background-color: $button-color-background-sure;
  cursor: pointer;
}

.options_message_push_active {
  border: $table-color-border-active;
  color: $font-color-highlight;
}

.new_messages {
  position: absolute;
  top: 120px;
  right: 36px;
  left: 36px;
  bottom: 36px;
  border: 1px $tabs-item-color-border solid;
  padding-top: 32px;
  display: none;
}

.options_content_active {
  display: block;
}

.options_message_title {
  line-height: 40px;
  font-size: 14px;
  color: $font-color-highlight;
  text-indent: 2em;

}

.options_message_box {
  width: 100%;
  height: 320px;
  overflow-x: hidden;
  overflow-y: auto;
}

#options_message_push_mine,
#options_message_mine_push {
  position: relative;
  width: 100%;
  min-height: 80%;
}

.option_message_timeline {
  position: absolute;
  top: 0;
  left: 90px;
  bottom: 12px;
  border: 2px $tabs-item-color-border solid;
  border-radius: 2px;
}

.option_message_list {
  position: relative;
  height: 48px;
  margin-left: 178px;
  line-height: 48px;
  margin-bottom: 12px;
}

.option_message_list_date {
  position: absolute;
  top: 12px;
  left: -165px;
  display: inline-block;
  width: 140px;
  line-height: 24px;
  background-color: $input-color-background;
  text-align: center;
  border-radius: 12px;
}

.option_message_list_number {
  margin-right: 4px;
}

.option_message_list_img {
  height: 36px;
  margin-right: 8px;
}

.option_message_list_people,
.option_message_list_name {
  margin: 0 4px;
  color: $font-color-highlight;
}

.option_message_list_check {
  margin-left: 20px;
  color: $font-color-highlight;
  font-style: normal;
  cursor: pointer;
}

.delegation_tab_btn {
  height: 32px;
  border: 1px $tabs-item-color-border solid;
  margin-right: 12px;
  padding: 0 8px;
  outline: none;
  color: $font-color;
  font-size: 14px;
  font-weight: 200;
  background-color: $tabs-item-color-background-default;
}

.delegation_tab_active {
  color: $font-color-highlight;
  border: 1px $tabs-item-color-border-active solid;
}

.delegation_content_box {
  margin-top: 24px;
  display: none;
  height:95%;/*c*/
}

#delegation_modify_option {
  display: flex;
  justify-content: space-between;
}

#delegation_modify_option .select_main_box{
  width: 125px;
}

.delegation_modify_option_list {
  position: relative;
  display: inline-block;
  // width: 32%;
  height: 25px;
  line-height: 25px;
}

#delegation_modify_group {
  width: 600px;
  margin: 0 auto;
}

#delegation_modify_group table {
  width: 100%;
}

#delegation_modify_group table thead td {
  border: 1px $table-color-border solid;
  border-collapse: collapse;
  text-align: center;
  line-height: 24px;
  background-color: $table-color-background-head;
