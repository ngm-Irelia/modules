#delegation_modify_option_ensure,
#delegation_modify_option_delete,
.delegation_modify_option_idcard_search {
  height: 25px;
  padding: 0 12px;
  outline: none;
  font-size: 14px;
  background-color: $button-color-background-sure;
}

#delegation_modify_option_ensure,
.delegation_modify_option_idcard_search{
  margin: 0 12px 0 24px;
  border: 1px $table-color-border-active solid;
}

#delegation_modify_option_ensure:hover,
.delegation_modify_option_idcard_search:hover{
  background-color: $button-color-background-sure-hover;
}

#delegation_modify_option_delete {
  border: 1px $button-color-border-cancel solid;
}

#delegation_modify_option_delete:hover {
  background-color: $button-color-background-cancel-hover;
}

#delegation_check_all,
.delegation_check_list {
  margin-right: 12px;
  margin-top: 5px;
  border: none;
  outline: none;
}

#delegation_check_all:before{
  background-color: $table-color-background-head;
  font-size: 16px;
}
.delegation_check_list:before {
  background-color: $table-color-background;
  font-size: 16px;
}

#option_log_filter,
#system_log_filter {
  padding: 10px;
  border: 1px dashed $tabs-item-color-border;
  flex-wrap: wrap;
}

#option_log_filter ul,
#system_log_filter ul {
  display: flex;
  justify-content: space-between;
}

.system_filter_list {
  position: relative;
  display: inline-block;
  white-space: nowrap;
}

.system_filter_title {
  margin-right: 12px;
  font-weight: 300;
  font-size: 14px;
  line-height: 36px;
  text-align: right;
  color: $font-color-highlight;
}

.clandar_box {
  position: relative;
  display: inline-block;
  margin: 0 12px;
}

.clandar_box:nth-child( 1 ) {
  margin-left: 0;
}

#option_filter_username,
#option_filter_user,
#option_filter_sys,
#option_filter_ip,
#system_filter_username,
#system_filter_type,
#system_filter_ip {
