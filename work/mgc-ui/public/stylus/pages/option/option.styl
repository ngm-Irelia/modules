  width: 110px;
  text-indent: 8px;
  background-color: $input-color-background;
  border: 1px solid $input-color-border;
}

#delegation_modify_option_select_input{
  height: 25px;
  width: 150px;
  text-indent: 8px;
  background-color: $input-color-background;
  border: 1px solid $input-color-border;
}

#option_time,
#system_time{
  height: 25px;
  width: 190px;
  text-indent: 8px;
  background-color: $input-color-background;
  border: 1px solid $input-color-border;
}

#option_filter_user,
#option_filter_sys,
#system_filter_type {
  display: inline-block;
  height: 25px;
  width: 110px;
  line-height: 25px;
  text-indent: 8px;
  font-size: 12px;
  background-color: $input-color-background;
  cursor: pointer;
}

#option_filter_type_lists,
#system_filter_type_lists {
  position: absolute;
  top: 32px;
  right: 0;
  width: 110px;
  z-index: 99;
  background-color: $input-color-background;
}

.select_type_list {
  text-indent: 8px;
  cursor: pointer;
}

.select_type_list:hover {
  background-color: $select-alert-color-background-hover;
}

#option_time_start,
#option_time_end,
#system_time_start,
#system_time_end {
  position: absolute;
  top: 3px;
  right: 3px;
  width: 20px;
  height: 20px;
  border: none;
  outline: none;
  padding: 0;
  background-color: $input-color-background;
  font-size: 16px;
}

#option_filter_ensure,
#system_filter_ensure,
#authority_add_ensure{
  width: 70px;
  height: 25px;
  margin-top: 5px;
  outline: none;
  background: $button-color-background-sure;
  border: 1px $table-color-border-active solid;
}

#option_filter_ensure:hover,
#system_filter_ensure:hover,
#authority_add_ensure:hover {
  background-color: $button-color-background-sure-hover;
}

#options_option_box,
#options_system_box,
#delegation_content {
  margin-top: 12px;
}

#option_log_lists,
#system_log_lists,
#delegation_detail_lists {
  height: 320px;
  position: relative;
  overflow-y: auto;
  overflow-x: hidden;
}

#option_content_head,
#system_content_head,
#delegation_detail_head {
  table-layout: fixed;
  background-color: $table-color-background-head;
}

/* 赫英迪加的 */
#option_content_head {
  width: 100%;
}

#delegation_detail_head {
  width: 100%;
}

/*赫英迪加完了*/
#option_content_head td,
#system_content_head td,
#delegation_detail_head td {
  line-height: 24px;
  color: $font-color-highlight;
}

#option_content_body,
#system_content_body,
#delegation_detail_body {
  table-layout: fixed;
}

#option_content_head td,
#option_content_body td,
#system_content_head td,
#system_content_body td,
#delegation_detail_head td,
#delegation_detail_body td {
  border: 1px $table-color-border solid;
  border-collapse: collapse;
  text-align: center;
}

#option_content_body td,
#system_content_body td,
#delegation_detail_body td {
  height: 35px;
  background: $table-color-background;
}

#delegation_detail_body td {
  line-height: 24px;
  background: $table-color-background;
}

#delegation_detail_head td:nth-child(1),
#delegation_detail_body td:nth-child(1) {
  text-indent: 2em;
  text-align: left;
}

#option_pagination_box,
#system_pagination_box,
#delegation_detail_pagination_box {
  margin-top: 70px;
  width: 100%;
  text-align: center;
}

#option_log_pagination,
#system_log_pagination,
#delegation_detail_pagination {
  display: inline-block;
}

#option_log_pagination a, #option_log_pagination span,
#system_log_pagination a, #system_log_pagination span,
#delegation_detail_pagination a, #delegation_detail_pagination span {
  padding: 2px 10px;
  font-size: 12px;
  background: $pagination-color-background;
  color: $pagination-color-text;
  border: 1px $pagination-color-border solid;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}

#option_log_pagination a,
#system_log_pagination a,
#delegation_detail_pagination a {
  cursor: pointer;
  text-decoration: none;
}

#option_log_pagination span,
#system_log_pagination span,
#delegation_detail_pagination span {
  cursor: default;
}

.prev {
  margin-right: 10px;
}

.next {
  margin-left: 10px;
}

#option_log_pagination .current,
#system_log_pagination .current,
#delegation_detail_pagination .current {
  background: $pagination-color-background-active;
  color: $pagination-color-text-active;
}

#option_log_pagination .btnactive,
#system_log_pagination .btnactive,
#delegation_detail_pagination .btnactive {
  background: $pagination-color-background-active;
  color: $pagination-color-text-active;
  cursor: default;
}

.pagination_page_box {
  display: inline-block;
  margin-left: 24px;
}

#option_page_num,
#system_page_num,
#delegation_detail_pagination_num {
  width: 32px;
  height: 20px;
  margin: 0 4px;
  background-color: $input-color-background;
  text-align: center;
  border: none;
}

#option_page_ensure,
#system_page_ensure,
#delegation_detail_pagination_ensure {
  width: 60px;
  height: 20px;
  margin-left: 12px;
  line-height: 16px;
  outline: none;
  background-color: $button-color-background-sure;
  border: 1px $table-color-border-active solid;
}

#option_page_ensure:hover,
#system_page_ensure:hover,
#delegation_detail_pagination_ensure:hover {
  background-color: $button-color-background-sure-hover;
}

