#delegation_modify_group table tbody td p {
  width: 558px;
  height: 20px;
  line-height: 20px;
  text-align: left;
  text-indent: 12px;
  outline-color: $font-color-highlight;
}

.delegation_delete_group_name,
.delegation_modify_group_name {
  position: absolute;
  top: 3px;
  width: 14px;
  height: 14px;
  font-size: 14px;
  margin: 0 10px;
  cursor: pointer;
}

.delegation_delete_group_name {
  right: 0;
}

.delegation_modify_group_name {
  right: 24px;
}

