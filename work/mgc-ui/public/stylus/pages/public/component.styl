h1, h2, h3, h4, h5, h6, div, p, ul, ol, li, table, tr, th, td {
  margin: 0;
  padding: 0;
}

em {
  font-style: normal;
}

a, s {
  text-decoration: none;
}

html, body {
  width: 100%;
  height: 100%;
  background: $body-color-background;
  color: $body-color-text;
  font-family: "微软雅黑";
  overflow: hidden;
  font-size: 12px;
}

input, textarea {
  outline-color: $input-color-ounline;
}

input::-ms-reveal {
  display: none;
}

input::-ms-clear  {
  display: none;
}

ul {
  list-style: none;
}

a {
  text-decoration: none;
}

.clearfix:before, .clearfix:after {
  content: "";
  display: block;
  clear: both;
}

.clearfix {
  zoom: 1;
}

/**** header ****/
#header {
  height: 50px;
  background: $header-color-background;
  background: linear-gradient(180deg, $header-bg-from, $header-bg-to);

  #header_logo {
    height: 50px;
    line-height: 50px;

    img{
      width: 40px;
      margin: 0 5px;
    }

    p {
      display: inline-block;
      line-height: 50px;
      font-size: 18px;
      font-weight: bolder;
      color: $header-color-text;
      vertical-align: top;
    }
  }
  //#selectBar
}
#selectBar {
  position: absolute;
  top: 0;
  right: 0;
  height: 50px;
  display: flex;
  align-items: center;

//.selectBar_div {
//  display: inline-block;
//  margin: 0 6px;
//  cursor: pointer;
//}  

  .selectBar_href {
    display: inline-block;
    margin: 0 6px;
    width: 40px;
    height: 50px;
    line-height: 50px;
    text-align: center;
    font-size: 28px;
    cursor: pointer;

    &::before {
      color: $header-color-icon;
    }

    &:hover::before {
      color: $header-color-icon-active;
    }

  }

  .selectBar_selected{
    &::before{
      color:$header-color-icon-active;
    }
  }

  #searchbox {
    position: relative;
    margin: 0 6px;

    #search_btn {
      border: none;
      outline: none;
      position: absolute;
      top: 2px;
      left: 2px;
      width: 24px;
      height: 24px;
      font-size: 18px;
      border-radius: 4px;
      background: $button-color-header-search-background;

      span::before {
      color: $header-color-icon;
    }
    }

    #search_input {
      width: 64px;
      height: 30px;
      padding-left: 20px;
      border: none;
      text-indent: 10px;
      border-radius: 3px;
      background: $input-color-header-search-background;
      color: $input-color-text;
    }

    .search_focus {
      width: 120px !important;
      transition: all 0.2s linear;
    }

    .search_blur {
      width: 56px;
      transition: all 0.2s linear;
    }
  }
}
#userBox {
  display: inline-block;
  cursor: pointer;

  #header_border {
    display: block;
    float: left;
    height: 40px;
    margin-top: 5px;
    border: 1px $header-color-border solid;
  }

  #user {
    display: block;
    float: left;
    height: 50px;
    margin: 0 6px;

    #userName {
      display: inline-block;
      line-height: 50px;
      font-size: 14px;
      font-weight: bold;
      color: $header-color-admin;
      text-indent: 4px;
      vertical-align: top;
      user-select: none;

      span {
        width: 15px;
        line-height: 50px;
        text-align: center;
        margin-right: 10px;

        &::before {
          font-size: 12px;
          color: $header-color-admin;
        }
      }
    }

    #personal_center {
      position: absolute;
      right: 0;
      background: $select-personcenter-color-background;
      border: 1px $select-personcenter-color-border solid;
      z-index: 1000;

      div {
        line-height: 24px;
        font-size: 12px;
        padding: 0 8px;
        cursor: pointer;

        &:hover {
          color: $select-color-text-hover;
        }

        &:first-of-type span {
          font-size: 14px;
        }

        span {
          display: inline-block;
          width: 20px;
          text-align: center;
          font-size: 12px;
          margin-right: 2px;

          &::before {
            color: $select-color-text-hover;
          }
        }
      }

      b {
        position: absolute;
        right: 48px;
        border-right: 8px transparent solid;
        border-left: 8px transparent solid;
      }

      .triangleDown{
        top: -8px;
        border-bottom: 8px $select-personcenter-color-background solid;
      }

      .triangleUp{
        bottom: -8px;
        border-top: 8px $select-personcenter-color-background solid;
      }


    }
  }
}
