  display: inline-block;
  width: 35%;
  color: $font-color;
}

.themeAlertBox .model_box input {
  background: none;
}

//用于显示历史记录
.search_history_box {
  position: absolute;
  top: 56px;
  right: 10px;
  z-index: 9;

  .icon-search-blue:hover:before {
    color: $his-font-hover-color;
  }
  a {
    color: $his-font-color;

    &:hover {
      color: $his-font-hover-color;
      text-decoration: none;
    }
  }
}
.search_history {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 40%;
  padding: 10px 15px;
  border: 1px solid $his-border-default-color;
  font-size: 14px;
  background-color: $his-bg-default-color;
  z-index: 1001;

  .search_history_close {
    position: absolute;
    top: 6px;
    right: 6px;
    cursor:pointer;
  }

  .search_history_head {
    border-bottom: 1px solid $his-head-bottom-border;

    h4 {
      margin-bottom: 1em;
      font-size: 16px;
      line-height: 1.5em;
      color: $BLUE;
      text-align: left;
    }

    input {
      width: 150px;
      height: 26px;
      margin: 2px 0;
      border: none;
      text-indent: 10px;
      border-radius: 3px;
      background: $his-input-default-bg;
    }
  }

  .ul_wrapper {
    min-height: 400px;
    max-height: 400px;
    overflow-y: hidden;

    ul {
      li {
        border-bottom: 1px solid $his-li-bottom-border;
        padding: 8px 0;
        cursor: pointer;
        padding-left:5px;

        &:nth-child(odd) {
          background-color: $his-li-odd-default-bg;
        }
        &:nth-child(even) {
          background-color: $his-li-even-default-bg;
        }
        &:nth-child(1) {
          margin-top: 4px;
        }

        .history_item_user {
          color: $his-font-color;
        }
        .history_item_time {
          color: $his-li-time;
        }
        .history_item_keyword {
          color: $his-font-color;
          text-decoration: underline;

          &:hover {
            color: $his-font-hover-color;
          }
        }
      }
    }
  }

  p {
    margin-top: 6px;
    color: $his-record-color;
  }
}

//搜索表达式匹配
.search_group {
  position: relative;
  display: inline-block;
  z-index: 1;

  .ul_wrapper {
    position: absolute;
    top: 40px;
    left: 0;
    width: 100%;
    border-radius: 4px;
    text-align: left;
    background-color: $exp-pane-bg;
    overflow-x: hidden;
    overflow-y: auto;
    z-index: 9;

    ul {
      padding-bottom: 4px;

      li {
        padding: 2px 8px;
        line-height: 18px;
        background-color: @background-color;
        cursor: pointer;
        color: $font-color;

        &:nth-child(1) {
          margin-top: 4px;
        }

        &:hover {
          background-color: $exp-li-bg-hover;
        }
      }
    }
  }
}

