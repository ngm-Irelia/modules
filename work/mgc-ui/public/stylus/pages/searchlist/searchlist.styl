@import "../public/component";
@import "../public/laydate";
@import "../../../js/public/jquery-custom-content-scroller/jquery.mCustomScrollbar";

#searchList_main {
  position: absolute;
  top: 50px;
  right: 0;
  bottom: 20px;
  left: 0;
  box-sizing: border-box;
  padding: 0 15px 15px 15px;
}

/*遮罩层*/
#search_shade {
  position: absolute;
  top: 50%;
  right: 50%;
  bottom: 50%;
  left: 50%;
  background-color: rgba(0, 0, 0, .6);
  z-index: 9;
  overflow: hidden;
}

/*搜索框*/
#searchList_main_search {
  width: 100%;
// height: 85px;
  margin: 10px 0;
  text-align: center;
  position: relative;
}

#searchList_search_box {
  display: inline-block;
  margin-top: 10px;
}

.search_group {

  .search_group_input {
    width: 320px;
    height: 40px;
    border: 1px $input-color-border solid;
    border-radius: 5px;
    font-size: 16px;
    padding-left: 8px;
    background-color: $input-color-background-search;
    color: $font-color;
  }

  .ul_wrapper {
    max-height: 300px;
    border:1px solid $border-color;
    background-color: $searchlist-wrapper-background-color;

    ul {
      li {
        background-color: @background-color;
        color:$font-color;

        &:hover {
          background-color: $searchlist-wrapper-li-hover-color;
        }
      }
    }
  }
}

.search_history_box,
#searchLsit_search_btn,
#searchLsit_search_advancedBtn {
  position: relative;
  width: 90px;
  height: 30px;
  border: none;
  outline: none;
  margin: -10px 8px 0 8px;
  vertical-align: middle;
  background: none;
  color: $font-color-highlight;
  font-size: 16px;
  cursor: pointer;

  .icon-button {
    position: absolute;
    left: 0;
    top: 0;
    width: 90px;
    height: 30px;
    line-height: 30px;

    span {
      font-size: 29px;
      &.path2 {
      //IE11兼容性
        position: absolute;
        top: 0;
        right: 0;
        font-size: 90px;
      }
    }

    p {
      position: absolute;
      left: 0;
      top: 0;
      display: inline-block;
      width: 100%;
      height: 100%;
      line-height: 27px;
      text-align: center;
    }
  }
}

// #searchLsit_search_advancedBtn {
//   margin-right: 0;
// }
#searchlist_searchhistory_btn{
  margin-right:0;
}

.search_history_box .icon-button a{
  position: absolute;
  left: 0;
  top: 0;
  display: inline-block;
  width: 100%;
  height: 100%;
  line-height: 30px;
  text-align: center;
}

.searchlist_tab_all{
  position:absolute;
  width:50px;
  margin-left:0;
}
.searchlist_tab{
  max-width:95%;
}
.searchlist_tab_entity{
  margin-left:50px;
}
.searchlist_tab b{
  margin-bottom: 5px;
}
#searchList_main_tab li {
  display: inline-block;
  vertical-align: bottom;
}

#searchList_main_tab .parent_classify,
#searchList_main_tab .child_classify,
.child_classify_more{
  display: inline-block;
  margin-right: 4px;
  padding: 0 8px;
  line-height: 28px;
  font-weight: normal;
  font-size: 14px;
  border: 1px $tabs-item-color-border solid;
  border-radius: 2px 2px 0 0;
  background-color: $tabs-item-color-background-active;
  cursor: pointer;
}
.child_classify_more{
  display:none;
}

#searchList_main_tab .tab_active {
  color: $font-color-highlight;
  border: 1px $tabs-item-color-border-active solid;
  background: $tabs-item-color-background-default;
  cursor: default;
}

#searchlist_main_content {
  position: absolute;
  top: 105px;
  right: 12px;
  bottom: 45px;
  left: 12px;
  overflow-y: auto;
  background: $searchlist-area-color-background;
  margin-top:10px;
}

#searchlist_nomessage {
  margin: 120px auto;
  padding-top: 54px;
  line-height: 48px;
  font-size: 16px;
  text-align: center;
  display: none;
}

#searchlist_nomessage:before {
  font-size: 80px;
}

#searchlist_makeLabel {
  color: $font-color-highlight;
  cursor: pointer;
  text-decoration: underline;
  user-select: none;
}

#searchList_main_box {
  box-sizing: border-box;
  position: relative;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
  -webkit-box-pack: justify;
  -ms-flex-pack: justify;
  justify-content: space-between;
  padding: 25px;
}

.searchList_main_content_box {
  width: 49.5%;
  min-width: 500px;
  height: 140px;
  position: relative;
  border: 1px $tag-color-border solid;
  border-radius: 10px;
  margin-bottom: 15px;
  background-color: $tag-color-background;
  color: $font-color;
  font-size: 14px;
}

.searchList_main_content {
  position: relative;
  height: 100%;
  border-radius: 10px;
  padding: 15px;
  background-color: $tag-color-background;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.search_go_detail {
  width: 95px;
  height: 35px;
  position: absolute;
  bottom: 15px;
  left: 15px;
  font-size: 16px;
  line-height: 35px;
  text-align: center;
  color: $searchlist-go-detail-color;
  cursor: pointer;
  display: none;
}
.search_go_detail .path1:before, .search_go_detail .path2:before{
  color:$searchlist-go-detail-before-color;
}
.search_go_detail .path1, .search_go_detail .path2 {
  position: absolute;
  left: 93px;
  top: 0;
  line-height: 36px;
  text-align: center;
}

.search_go_detail .path1:before, .search_go_detail .path2:before {
  font-size: 30px;
}

.searchList_content_img {
  display: inline-block;
  position: absolute;
  top: 20px;
  left: 20px;
  border-radius: 50%;
  vertical-align: middle;
  overflow: hidden;
}

.searchList_content_img img {
  width: 80px;
  height: 80px;
  background-color: $image-background-color;
}

.searchList_content_text {
  position: absolute;
  top: 30px;
  left: 120px;
  right: 4px;
  height: 95px;
  overflow-y: hidden;
  display: inline-block;
}

.searchList_content_text>div:nth-of-type(1)>div>div {
  min-width: 50%;
  max-width: 90%;
  float: left;
  line-height: 30px;
  overflow: hidden;
  text-overflow: ellipsis;
}

.searchList_content_text>div:nth-of-type(1)>div>div p {
  display: inline;
}

.searchList_content_text>div:nth-of-type(1)>div>div p span.dot {
  color: $font-color-highlight;
  font-size: 12px;
}

.searchList_content_text .makeEntity {
  text-decoration: none;
}

.searchList_content_gis, .searchList_content_topology {
  width: 24px;
  height: 24px;
  position: absolute;
  top: 8px;
  right: 16px;
  cursor: pointer;
  z-index: 1;
  font-size: 19px;
}

.searchList_content_topology {
  top: 10px;
  right: 10px;
}

.searchList_content_gis {
  top: 10px;
  right: 35px;
}

.searchList_main_content_box:hover .searchList_main_content {
  width: 105%;
  min-width: 600px;
  height: 180px;
  box-sizing: border-box;
  position: absolute;
  top: 0;
  z-index: 10;
  border: 2px $searchlist-textarea-color-border solid;
  margin-bottom: 15px;
  padding: 15px;
  white-space: normal;
}

.searchList_main_content:hover .search_go_detail {
  display: inline-block;
}

.searchList_main_content_box:hover .searchList_content_text {
  height: 125px;
  overflow-y: auto;
  overflow-x: hidden;
  display: inline-block;
  vertical-align: middle;
}

.searchList_main_content_box:hover .searchList_content_text span {
  font-weight: bold;
  color: $font-color-highlight;
}

#searchlist_pagination_box {
  position: absolute;
  bottom: 0;
  width: 100%;
  text-align: center;
}

#searchlist_pagination {
  display: inline-block;
}

#searchlist_pagination a, #searchlist_pagination span {
  padding: 4px 12px;
  font-size: 16px;
  font-weight: bold;
  background: $pagination-color-background;
  color: $pagination-color-text;
  border: 1px $pagination-color-border solid;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}

#searchlist_pagination a {
  cursor: pointer;
  text-decoration: none;
}

#searchlist_pagination span {
  cursor: default;
}

.prev {
  margin-right: 10px;
}

.next {
  margin-left: 10px;
}

#searchlist_pagination .current {
  background: $pagination-color-background-active;
  color: $pagination-color-text-active;
}

#searchlist_pagination .btnactive {
  background: $pagination-color-background-active;
  color: $pagination-color-text-active;
  cursor: default;
}

#searchlist_loading {
  position: absolute;
  top: 280px;
  left: 50%;
  text-align: center;
  transform: translate(-50%, -50%);
  display: none;
}

#searchlist_loading img {
  width: 400px;
}

