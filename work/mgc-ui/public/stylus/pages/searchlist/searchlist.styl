@media screen and (min-width: 1200px) {
  .searchList_main_content_box:nth-child(2n+1):hover .searchList_main_content {
    left: 0;
  }

  .searchList_main_content_box:nth-child(2n):hover .searchList_main_content {
    right: 0;
  }
}

@media screen and (min-width: 926px) and (max-width: 1199px) {
  .searchList_main_content_box {
    width: 600px;
    height: 140px;
    position: relative;
    border: 1px $tag-color-border solid;
    border-radius: 10px;
    margin: 0 auto 15px;
    background-color: $tag-color-background;
    color: $font-color;
    font-size: 14px;
  }

  .searchList_main_content_box:hover .searchList_main_content {
    width: 650px;
    height: 170px;
    box-sizing: border-box;
    position: absolute;
    top: 0;
    left: -25px;
    z-index: 10;
    border: 2px $searchlist-textarea-color-border solid;
    margin-bottom: 15px;
    padding: 15px;
    white-space: normal;
  }

  .searchList_main_content_box:hover .searchList_content_text {
    height: 125px;
    overflow-y: auto;
    display: inline-block;
    vertical-align: middle;
  }

}

@media screen and (min-width: 800px) and (max-width: 925px) {
  .searchList_main_content_box {
    width: 500px;
    height: 140px;
    position: relative;
    border: 1px $tag-color-border solid;
    border-radius: 10px;
    margin: 0 auto 15px;
    background-color: $tag-color-background;
    color: $font-color;
    font-size: 14px;
  }

  .searchList_main_content_box:hover .searchList_main_content {
    width: 550px;
    height: 160px;
    box-sizing: border-box;
    position: absolute;
    top: 0;
    left: -50px;
    z-index: 10;
    border: 2px $searchlist-textarea-color-border solid;
    margin-bottom: 15px;
    padding: 15px;
    white-space: normal;
  }

  .searchList_main_content_box:hover .searchList_content_text {
    height: 125px;
    overflow-y: auto;
    display: inline-block;
    vertical-align: middle;
  }
}

@media screen and (max-width: 799px) {
  #searchList_header_nav {
    display: none;
  }

  .searchList_main_content_box {
    width: 500px;
    height: 140px;
    position: relative;
    border: 1px $tag-color-border solid;
    border-radius: 10px;
    margin: 0 auto 15px;
    background-color: $tag-color-background;
    color: $font-color;
    font-size: 14px;
  }

  .searchList_main_content_box:hover .searchList_main_content {
    width: 530px;
    height: 156px;
    box-sizing: border-box;
    position: absolute;
    top: 0;
    left: -50px;
    z-index: 10;
    border: 2px $searchlist-textarea-color-border solid;
    margin-bottom: 15px;
    padding: 15px;
    white-space: normal;
  }

  .searchList_main_content_box:hover .searchList_content_text {
    height: 125px;
    overflow-y: auto;
    display: inline-block;
    vertical-align: middle;
  }
}

