//gis和拓扑toolTip
.tipStyle{
  height: 17px;
  line-height: 16px;
  padding-left: 3px;
  padding-right: 3px;
  position: absolute;
  font-size: 12px;
  text-align: center;
  background-color: $totip-tool-color-background;
  color: black;
  top:36px;
}

.search_history {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 40%;
  padding: 10px 15px;
  border: 1px solid $his-border-default-color;
  font-size: 14px;
  background-color: $searchlist-history-background-color;
  z-index: 1001;

  .search_history_close {
    position: absolute;
    top: -12px;
    right: 6px;
    cursor:pointer;
  }

  .search_history_head {
    border-bottom: 1px solid $searchlist-history-bottom-color;

    h4 {
      margin-bottom: 1em;
      font-size: 16px;
      line-height: 1.5em;
      color: $BLUE;
      text-align: left;
    }

    input {
      width: 150px;
      height: 26px;
      margin: 2px 0;
      border: none;
      text-indent: 10px;
      border-radius: 3px;
      background: $searchlist-history-input-background-color;
    }
  }

  .ul_wrapper {
    min-height: 400px;
    max-height: 400px;
    overflow-y: auto;

    ul {
      li {
        border-bottom: 1px solid $searchlist-history-bottom-color;
        padding: 8px 0;
        cursor: pointer;
        padding-left:5px;

        &:nth-child(odd) {
          background-color: $searchlist-history-li-odd-background-color;
        }
        &:nth-child(even) {
          background-color: $searchlist-history-li-even-background-color;
        }
        &:nth-child(1) {
          margin-top: 4px;
        }

        .history_item_user {
          color: $his-font-color;
        }
        .history_item_time {
          color: $his-li-time;
        }
        .history_item_keyword {
          color: $his-font-color;
          text-decoration: underline;

          &:hover {
            color: $his-font-hover-color;
          }
        }
      }
    }
  }

  p {
    margin-top: 6px;
    color: $his-record-color;
  }
}