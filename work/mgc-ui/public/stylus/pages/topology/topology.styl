svg {
  cursor: default;
}

svg:not(.active):not(.ctrl) {
  cursor: crosshair;
}

.time_axis .tick text ,.legendTitle{
  fill: $font-color;
}

.topo_network{
  background-color:$body-color-background;
}

.tool-parent-right {
  padding: 0 0 0 0;
}

.thumbnail_small {
  cursor: pointer;
  width: 12px;
  height: 12px;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
}

.gbox {
  cursor: pointer;
}

.thumbnail_big {
  cursor: pointer;
  width: 16px;
  height: 16px;
  position: absolute;
  bottom: 5px;
  right: 5px;
  display: none;
}

.thumbnail_big:before {
  font-size: 16px;
}

.brushRect:hover {
  cursor: pointer;
}

#topo_mapMoveBar:hover {
  cursor: pointer;
}

g.node:hover, .topoMenu:hover, .topoMenu_one:hover, .topoMenu_two:hover, .topoMenu_three:hover {
  cursor: pointer;
}

.nodes_select {
  stroke-width: 5px;
}

.clear {
  clear: both;
}

line {
  fill: $topology-line-fill ;
  stroke:$topology-line-stroke ;
  stroke-width: 2px;
}

.pie text {
  fill: $topology-pie-text-fill;
  font-weight: bold;
}

path.linktext {
  cursor: pointer;
}

.resolved {
  stroke-dasharray: 0, 2 1;
}

// marker#licensing {
//   fill:$topology-marker-licensing-fill ;
// }

// path.link.licensing {
//   stroke:$topology-path-link-licensing-stroke;
// }

path.link.resolved {
  stroke-dasharray: 0, 2 1;
}

path.overlay {
  cursor: pointer;
  opacity: 0;
}

g.node.selected circle, g.node.filtered circle {
  filter: url(#blue-glow);
}

path.links.hidden {
  stroke-width: 0;
}

path.dragline {
  stroke-width: 1px;
  stroke: $topology-path-dragline-stroke;
}

.relationSvg text {
  font: 12px "微软雅黑";
  pointer-events: none;
  fill: $font-color;
}

.relationSvg textPath {
  fill: $font-color;
}

text.shadow {
  stroke: none;
  stroke-width: 3px;
  stroke-opacity: .8;
}

.topoMenu_inring {
  stroke-width: 1px;
  fill: $topo-fill-color-inring;
  stroke: $topo-stroke-color-inring;
}

.topoMenu_inring_over{
  fill: $topo-fill-color-inring-over;
}

.topoMenu_inring_enter{
  fill: $topo-fill-color-inring-enter;
}

.topoMenu_inring_onmenu{
  fill: $topo-fill-color-inring-onmenu;
}

.topoMenu_outring{
  fill: $topo-fill-color-outring;
}

.topoMenu_outring_over{
  fill: $topo-fill-color-outring-over;
}

.toponode_text{
  fill: $font-color;
}

.brush .extent, .topo_brush .extent {
  fill: $topo-rect-extent;
  stroke-width:0;
  fill-opacity: 0.15;
  shape-rendering: crispEdges;
}

.topo_brush .extent {
  stroke: $topo-rect-extent;
  stroke-width:2px;
  fill-opacity: 0.3;
}

.resize rect {
  visibility: visible;
  stroke: $topology-resize-rect-stroke;
  stroke-width: 2px;
}

.time_selected {
  fill: $topology-time_selected-fill;
}
.time_axis path,
.time_axis line{
  fill: none;
  stroke:$topology-time_axis-pathline-stroke;
  shape-rendering: crispEdges;
}
.timeline-axis path,
.timeline-axis line{
  fill: none;
  stroke: $topology-timeline-axis-pathline-stroke;
  shape-rendering: crispEdges;
}
.time_axis text ,.timeline-axis text{
  font-family: sans-serif;
  font-size: 11px;
}

.dot {
  stroke: $BLACK;
}

.legend {
  padding: 5px;
  box-shadow: 2px 2px 1px $topology-legend-box-shadow ;
}

