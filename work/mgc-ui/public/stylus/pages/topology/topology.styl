// 思维导图
#mindmap_pane {
  position: absolute;
  width: 100%;
  height: 14%;
  background-color:$topology-mindmap_pane-backgroundcolor ;
  opacity :0.5;
  z-index: 103;
  display: none;

  .mindmap {
    cursor: default;

    .g_circles {
      circle {
        fill: $mm-final-circle-bgcolor;
        &:nth-child(1) {
          fill: $mm-initial-circle-bgcolor;
        }

        &:hover {
          cursor: pointer;
        }
      }
    }

    .g_lines {
      path {
        stroke: $mm-final-path-color;
        stroke-width: 2px;
        stroke-linejoin: round;
      }
    }
    .g_lines_blue {
      path {
        stroke: $mm-initial-path-color;
        stroke-width: 2px;
        stroke-linejoin: round;
        stroke-dasharray: 375;
        stroke-dashoffset: 375;
      }
    }

    #symbol {
      path {
        fill: currentColor;
        stroke-linejoin:round;
        stroke-linecap:round;
        stroke: currentColor;
      }
      rect {
        stroke-linejoin:round;
        stroke-linecap:round;
      }
    }
    .g_arrows {
      use {
        transition: all .6s .2s;
      }
      .coarrow_initial {
        fill: $mm-initial-btn-bgcolor;
        color: $mm-btn-bgcolor;
      }
      .coarrow_final {
        fill: $mm-btn-bgcolor;
        color: $mm-coarrow_final-color;
      }
    }

    //.g_arrows {
    //  path {
    //    stroke: $mm-executed-path-color;
    //    stroke-width: 2px;
    //    stroke-linejoin: round;
    //    fill: $mm-executed-path-color;
    //  }
    //}
    //.g_arrows_blue {
    //  path {
    //    stroke: $mm-executed-path-color;
    //    stroke-width: 2px;
    //    stroke-linejoin: round;
    //    stroke-dasharray: 100;
    //    stroke-dashoffset: 100;
    //    fill: none;
    //  }
    //}

    .g_labels {

      text {
        fill: $topology-g_labels-text-fill;
        text-anchor: middle;
        font-size: 14px;

        &:hover {
          cursor: pointer;
        }
      }
    }

  }

  .g_tooltip {
    position: absolute;
    top: 0;

    .tooltip {
      position: absolute;
      width: 120px;
      height: auto;
      border: 1px solid $mm-tooltip-border-color;
      border-radius: 4px;
      background-color: $mm-tooltip-bgcolor;
      color: $mm-tooltip-font-color;
      opacity: 0;
      transition: opacity .5s, left .5s, top .5s;

      &::after {
        content: '';
        position: absolute;
        top: -8px;
        left: 20px;
        width: 12px;
        height: 12px;
        border: 1px solid $mm-tooltip-border-color;
        border-width: 1px 0 0 1px;
        background-color: $mm-tooltip-bgcolor;
        transform: rotate(45deg);
        //border: 8px solid transparent;
        //border-bottom-color: #ccc;
      }
    }
  }
}

