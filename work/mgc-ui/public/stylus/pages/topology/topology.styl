                border-radius: 5px;
            }
        }
        .trajectory_item{
            width: 100%;
            min-height: 100px;
            
            .trajectory_compare{
                position: absolute;
                left: 50%;
                transform: translate(-50%, 0);
                width: 2px;
                height: 100%;
                background: $topology-trajectory_timecompare-backgroundcolor ;
            }

            .trajectory_border_color{
              border: $topology-trajectory_item_border;
            }

            .trajectory_border_color_selected{
              border: $topology-trajectory_item_border_selected;
            }

            .trajectory_item_box_left{
                float: left;
                width: 100%;
                border-right: 1px solid $topology-trajectory_item_box_left-border ;
                padding: 0 0 20px 10px;

                .trajectory_item_left{
                    position: relative;
                    float: right;
                    margin-right: 40px;
                    border-radius: 10px;
                    background: $topology-trajectory_item_left-backgroundcolor;
                    padding: 10px;
                    
                    .triangle_border{
                        position: absolute;
                        top: 0;
                        right: -10px;
                        width: 0;
                        height: 0;
                        border-width: 10px 0 10px 10px;
                        border-style: solid;
                        border-color: transparent transparent transparent $topology-trajectory_item_border;
                        margin: 15px auto;
                    }
                    .triangle_border:after{
                        content: "";
                        position: absolute;
                        top: -25px;
                        right: 1px;
                        width: 0;
                        height: 0;
                        border-width: 10px 0 10px 10px;
                        border-style: solid;
                        border-color: transparent transparent transparent $topology-triangle_border-borderColor;
                        margin: 15px auto;
                    }
                    .triangle_border_selected{
                        position: absolute;
                        top: 0;
                        right: -10px;
                        width: 0;
                        height: 0;
                        border-width: 10px 0 10px 10px;
                        border-style: solid;
                        border-color: transparent transparent transparent $topology-trajectory_item_border_selected;
                        margin: 15px auto;
                    }
                    .triangle_border_selected:after{
                        content: "";
                        position: absolute;
                        top: -25px;
                        right: 1px;
                        width: 0;
                        height: 0;
                        border-width: 10px 0 10px 10px;
                        border-style: solid;
                        border-color: transparent transparent transparent $topology-triangle_border-borderColor;
                        margin: 15px auto;
                    }
                    .dot_border{
                        position: absolute;
                        top: 20px;
                        right: -46px;
                        width: 10px;
                        height: 10px;
                        background: $topology-dot_border-backgroundcolor;
                        border-radius: 50%;
                    }
                    .trajectory_item_content{
                        display: flex;
                        align-items: center;
                        justify-content: center;
                        word-wrap:break-word;  
                        word-break:break-all;  
                        overflow: hidden; 
                        text-align: left;

                        .trajectory_icon{
                            font-size: 24px;
                            margin: 0 10px;

                            &:before{
                            color: $icon-color-highlight;  
                            }
                            &>.path1:before{
                            color: $icon-color-highlight;  
                            }
                            &>.path2:before{
                            color: $icon-color-highlight;  
                            }
                        }
                        .trajectory_item_text{
                            margin: 0 5px;
                        }
                    }
                } 
            }
            
            .trajectory_item_box_right{
                float: right;
                width: 100%;
                border-left: 1px solid $topology-trajectory_item_box_right-borderLeft;
                padding: 0 10px 20px 0;
                
                .trajectory_item_right{
                    position: relative;
                    float: left;
                    margin-left: 40px;
                    // border: $topology-trajectory_item_right-border;
                    border-radius: 10px;
                    background: $topology-trajectory_item_right-backgroundcolor;
                    padding: 10px;
                    
                    .triangle_border{
                        position: absolute;
                        top: 0;
                        left: -10px;
                        width: 0;
                        height: 0;
                        border-width: 10px 10px 10px 0;
                        border-style: solid;
                        border-color: transparent $topology-trajectory_item_border transparent transparent;
                        margin: 15px auto;
                    }
                    .triangle_border:after{
                        content: "";
                        position: absolute;
                        top: -25px;
                        left: 1px;
                        width: 0;
                        height: 0;
                        border-width: 10px 10px 10px 0;
                        border-style: solid;
                        border-color: transparent $topology-triangle_border-borderColor transparent transparent;
                        margin: 15px auto;
                    }
                    .triangle_border_selected{
                        position: absolute;
                        top: 0;
                        left: -10px;
                        width: 0;
                        height: 0;
                        border-width: 10px 10px 10px 0;
                        border-style: solid;
                        border-color: transparent $topology-trajectory_item_border_selected transparent transparent;
                        margin: 15px auto;
                    }
                    .triangle_border_selected:after{
                        content: "";
                        position: absolute;
                        top: -25px;
                        left: 1px;
                        width: 0;
                        height: 0;
                        border-width: 10px 10px 10px 0;
                        border-style: solid;
                        border-color: transparent $topology-triangle_border-borderColor transparent transparent;
                        margin: 15px auto;
                    }
                    .dot_border{
                        position: absolute;
                        top: 20px;
                        left: -45px;
                        width: 10px;
                        height: 10px;
                        background: $topology-dot_border-backgroundcolor ;
                        border-radius: 50%;
                    }
                    .trajectory_item_content{
                        display: flex;
                        align-items: center;
                        justify-content: center;
                        word-wrap:break-word;  
                        word-break:break-all;  
                        overflow: hidden;
                        text-align: left; 

                        .trajectory_icon{
                            font-size: 24px;
                            margin: 0 10px;

                            &:before{
                            color: $icon-color-highlight;  
                            }
                            &>.path1:before{
                            color: $icon-color-highlight;  
                            }
                            &>.path2:before{
                            color: $icon-color-highlight;  
                            }
                        }
                        .trajectory_item_text{
                            margin: 0 5px;
                        }
                    }
                }
            }
        }
        .clearFloat:after{
            display:block;
            content: ' ';
            clear:both;
            overflow: hidden;
        }
    }
    #trajectory_nodata{
        position: absolute;
        top: 50%;
        left: 46%;
    }
